{
  "name": "solid-repl",
  "description": "A REPL for SolidJS",
  "author": "Alexandre Mouton-Brady",
  "license": "MIT",
  "version": "0.0.0",
  "homepage": "https://github.com/ryansolid/solid-repl#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansolid/solid-repl.git"
  },
  "type": "module",
  "main": "./dist/cjs/solid-repl.js",
  "module": "./dist/esm/solid-repl.js",
  "types": "./dist/types/solid-repl.d.ts",
  "exports": {
    ".": "./dist/esm/solid-repl.js",
    "./cjs": "./dist/cjs/solid-repl.js",
    "./raw": "./dist/raw/solid-repl.jsx"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c && tsc",
    "dev": "rollup -c -w",
    "format": "prettier --write \"src/**/*.{jsx,tsx,ts,js,json}\"",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "solid-js": "^0.22.7"
  },
  "dependencies": {
    "@babel/standalone": "^7.12.9",
    "@codemirror/next": "^0.14.0",
    "babel-preset-solid": "^0.22.7",
    "oceanwind": "^0.9.0",
    "rollup": "^2.33.3",
    "solid-js": "^0.22.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/babel__standalone": "^7.1.3",
    "prettier": "^2.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.1.2"
  }
}
