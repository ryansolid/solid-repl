{
  "name": "solid-repl",
  "description": "A REPL for SolidJS",
  "author": "Alexandre Mouton-Brady",
  "license": "MIT",
  "version": "0.1.1",
  "homepage": "https://github.com/ryansolid/solid-repl#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansolid/solid-repl.git"
  },
  "main": "dist/cjs/solid-repl.js",
  "module": "dist/esm/solid-repl.js",
  "types": "dist/types/solid-repl.d.ts",
  "exports": {
    ".": "./dist/raw/solid-repl.jsx",
    "./cdn": {
      "import": "./dist/esm/solid-repl.js",
      "require": "./dist/cjs/solid-repl.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c && tsc",
    "dev": "rollup -c -w",
    "format": "prettier --write \"src/**/*.{jsx,tsx,ts,js,json}\"",
    "test": "parcel playground/index.html",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "solid-js": "^0.22.8"
  },
  "dependencies": {
    "lz-string": "^1.4.4",
    "solid-js": "^0.23.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/lz-string": "^1.3.34",
    "babel-preset-solid": "^0.23.5",
    "parcel": "^2.0.0-nightly.486",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.1.3"
  }
}
